  "scripts": {
    "start": "run-script-os",
    "start:win32": "yarn nx run main:serve --port %PORT% --configuration=development",
    "start:default": "yarn nx run main:serve --port $PORT --configuration=development",
    "start:local": "yarn nx run main:serve --port 4300 --configuration=development",
    "lint": "yarn nx run-many --target=lint --all=true",
    "lint:fix": "yarn nx run-many --target=lint --all=true -- --fix",
    "stylelint": "yarn nx run-many --target=stylelint --all=true",
    "stylelint:fix": "yarn nx run-many --target=stylelint --all=true -- --fix",
    "test": "yarn nx run-many --target=test --all=true",
    "check": "yarn codegen && yarn build && yarn test && yarn lint:fix && yarn stylelint:fix",
    "cache:clear": "yarn nx clear-cache",
    "codegen": "yarn codegen:schema && yarn nx run-many --target=graphql-codegen --all=true",
    "codegen:schema": "yarn cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 BACKEND_URL=https://localhost:12000/api/graphql graphql-codegen --config codegen.ts"
  },

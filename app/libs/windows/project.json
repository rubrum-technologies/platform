{
  "name": "windows",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/windows/src",
  "prefix": "r",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/windows/ng-package.json",
        "tailwindConfig": "libs/windows/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/windows/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/windows/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/windows/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "stylelint": {
      "executor": "nx-stylelint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/windows/**/*.less"]
      }
    }
  }
}
